<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.httptest_2">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoadWebView"/>
        <activity android:name=".DownloadImage"/>
        <activity android:name=".Http_Get_and_Post_Test"/>
        <activity android:name=".HttpClientTest"/>
        <activity android:name=".JsonTest.JSONTest"/>
        <activity android:name=".xmlTest.HttpXmlTest"/>
        <activity android:name=".MutiThreadDownload.MutiThreadDownloadActivity"/>

    </application>

</manifest>